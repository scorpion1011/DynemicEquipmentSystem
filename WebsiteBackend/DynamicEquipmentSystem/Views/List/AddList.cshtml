@model DynamicEquipmentSystem.ViewModels.AddListViewModel
@{
    ViewBag.Title = "Додати список";
}
<form asp-controller="List">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Name" class="control-label">Назва списку</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Додати" class="btn btn-default" />
    </div>
</form>